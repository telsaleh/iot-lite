  "@type" : [ "http://www.w3.org/2002/07/owl#AnnotationProperty" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "Relative Altitude can be used in place of an absolute altitude, especially in buildings, where it makes more sense to locate by levels or floors. For example the value \"0\" would indicate \"ground floor\", and \"1\" would indicate \"1st floor\" and so on."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#domain" : [ {
